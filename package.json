{
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "jest  --env=jsdom",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "prettier": "eslint ./ --fix",
    "eslint": "eslint ./",
    "stylelint": "stylelint ./**/*.js",
    "prestylecheck": "run-s prettier",
    "stylecheck": "run-p stylelint eslint"
  },
  "dependencies": {
    "compression": "1.7.1",
    "express": "4.16.2",
    "http-proxy": "1.16.2",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "next": "4.2.1",
    "next-redux-wrapper": "1.3.5",
    "react": "16.2.0",
    "react-disqus-comments": "1.1.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-dom": "16.2.0",
    "react-ga": "2.4.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-simplemde-editor": "3.6.11",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "remarkable": "1.7.1",
    "reselect": "3.0.1",
    "semantic-ui-react": "0.77.1",
    "styled-components": "2.4.0",
    "superagent": "3.8.2"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-styled-components": "1.4.0",
    "cross-env": "5.1.3",
    "enzyme": "3.3.0",
    "eslint": "4.15.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-revolut-react": "^0.0.2",
    "eslint-plugin-prettier": "2.4.0",
    "eslint-plugin-react-app": "1.0.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.5.1",
    "jest": "22.0.4",
    "jest-enzyme": "4.0.2",
    "jest-styled-components": "4.9.0",
    "npm-run-all": "4.1.2",
    "prettier": "1.9.2",
    "prop-types": "15.6.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.2.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "collectCoverageFrom": [
      "./**/*.{js,jsx}",
      "!./**/*/index.{js,jsx}",
      "!./.next"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-styled-components"
    ],
    "syntax": "scss",
    "rules": {
      "block-opening-brace-space-after": "never-single-line",
      "block-closing-brace-space-before": "never-single-line"
    }
  }
