{
  "extends": [
    "react-app",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/no-string-refs": 2,
    "react/jsx-boolean-value": 1,
    "react/prefer-es6-class": 2,
    "react/jsx-no-bind": 2,
    "prettier/prettier": [1 , {"singleQuote": true, "trailingComma": "all", "semi": false}],
    "prefer-const": 2
  }
}